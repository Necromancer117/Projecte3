{
  "type": "module",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jquery": "^3.6.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "tailwindcss": "^3.2.2",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "scripts": {
    "buildcss": "postcss App/css/styles.css --output public/styles.css",
    "watchcss": "postcss App/css/styles.css --output public/styles.css --watch",
    "buildjs": "webpack",
    "watchjs": "webpack --watch",
    "build": "npm-run-all -p buildcss buildjs",
    "watch": "npm-run-all -p watchcss watchjs",
    "prodjs": "webpack --mode production",
    "prod": "npm-run-all -p buildcss prodjs",
    "serve": "cd public && php -S localhost:8080 && echo 'Server started on http://localhost:8080'",
    "start": "npm-run-all -p watch serve",
    "test": "node --experimental-vm-modules node_modules/.bin/jest --env=jsdom"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "dependencies": {
    "chart.js": "^4.0.1",
    "datatables": "^1.10.18",
    "datatables.net": "^1.13.1",
    "datatables.net-dt": "^1.13.1",
    "jsdom": "^20.0.2",
    "leaflet": "^1.9.2"
  }
}